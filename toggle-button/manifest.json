{
  "name": "Better Github: toggle button",
  "version": "0.1",
  "manifest_version": 2,
  "description": "toggles deleted lines in diffs",
  "permissions": [
    "tabs", "http://www.github.com/*"
    ],
  "content_scripts": [
    {
      "matches": ["https://*.github.com/*"],
      "run_at": "document_end",
      "js": ["jquery.js", "github.js"]
    }
  ]
}
